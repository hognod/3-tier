[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/<Working Directory>
ExecStart=/home/ubuntu/<Venv Directory>/bin/gunicorn \
        --workers 2 \
        --bind 0.0.0.0:8000 \
        <Project Directory>.wsgi:application

[Install]
WantedBy=multi-user.target